{
	"name": "david63/inactivememb",
	"type": "phpbb-extension",
	"description": "Removes inactive members from the Memberlist.",
	"version": "2.1.0-RC1",
	"time": "2019-06-12",
	"keywords": [
		"phpbb",
		"extension",
		"memberlist"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Extension Developer"
		}
    ],
	"require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "Inactive members",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.7"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "inactivememb_version_file.json",
			"ssl": false
    	}
	}
}
